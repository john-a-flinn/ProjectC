generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model joblist {
  mfr          String? @db.VarChar(50)
  type_name    String? @db.VarChar(50)
  type_id      Int?
  style_name   String? @db.VarChar(50)
  style_id     String? @db.VarChar(50)
  color_number Int?
  colorname    String? @db.VarChar(50)
  size         String? @db.VarChar(50)
  id           Int     @id @default(autoincrement())
}
